<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.inventory.model">

	<class name="InventoryStoreDrugIndent" table="inventory_store_drug_indent" batch-size="500">
		<id name="id" type="int" column="id" unsaved-value="0">
				<generator class="native" />
		</id>
	    <many-to-one name="store" class="org.openmrs.module.inventory.model.InventoryStore"
	        column="store_id" cascade="none" lazy="false" />
	        
	    <many-to-one name="transaction" class="org.openmrs.module.inventory.model.InventoryStoreDrugTransaction"
	        column="transaction_id" cascade="none" lazy="false" />
		<property name="name" column="name"  />
		<property name="createdOn" column="created_on"  />	
		<property name="subStoreStatus" column="sub_store_status"  />
		<property name="mainStoreStatus" column="main_store_status"  />
	</class>
</hibernate-mapping>
